<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.vteconomiccalendar" version="1.1.3" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Velocity Trade Economic Calendar</name>
    <description>
        Velocity Trade Economic Calendar
    </description>
    <author email="vtcec@velocitytrade.com" href="http://velocitytrade.com/">
        Velocity Trade Team
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-targetSdkVersion" value="28" />
    <preference name="deployment-target" value="8.0" />
    <preference name="phonegap-version" value="cli-7.1.0" />
    <plugin name="cordova-plugin-device" source="npm" spec="~1.1.7" />
    <plugin name="cordova-plugin-dialogs" source="npm" spec="~1.2.1" />
    <plugin name="cordova-plugin-network-information" source="npm" spec="~1.2.1" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.2" />
    <plugin name="cordova-plugin-geolocation" version="4.0.2" />
    <icon src="images/favicon.png"  width="1024" height="1024" />
    <gap:splash src="images/favicon.png" />
    <platform name="ios">
        <icon height="1024" platform="ios" src="images/favicon-1024.png" width="1024" />
        <icon height="120" platform="ios" src="images/favicon-120.png" width="120" />
        <icon height="152" platform="ios" src="images/favicon-152.png" width="152" />
        <icon height="72" platform="ios" src="images/favicon-72.png" width="72" />
        <icon height="76" platform="ios" src="images/favicon-76.png" width="76" />
        <icon height="40" platform="ios" src="images/favicon-40.png" width="40" />
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
